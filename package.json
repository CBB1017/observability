{
  "name": "jest-sample",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:dev": "npx cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/app.ts",
    "start:prod": "npx cross-env NODE_ENV=production ts-node src/app.ts",
    "migration:create": "ts-node -r tsconfig.json ./node_modules/typeorm/cli.js migration:create ./src/migrations/Migration",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource ormconfig.ts",
    "migration:gen": "npx typeorm migration:generate ./src/migrations -d ./dist/config/database.js",
    "migration:run": "npx typeorm migration:run -d ./dist/config/database.js",
    "migration:revert": "npm run typeorm migration:revert",
    "build": "tsc",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation": "^0.55.0",
    "@opentelemetry/instrumentation-fastify": "^0.42.0",
    "@opentelemetry/instrumentation-http": "^0.55.0",
    "@opentelemetry/sdk-node": "^0.55.0",
    "@types/node": "^22.9.0",
    "@types/redis": "^4.0.10",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@opentelemetry/auto-instrumentations-node": "^0.53.0",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.55.0",
    "@opentelemetry/exporter-prometheus": "^0.55.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.55.0",
    "axios": "^1.7.8",
    "dotenv": "^16.4.5",
    "expect": "^29.7.0",
    "fastify": "^5.1.0",
    "fastify-metrics": "^12.1.0",
    "fastify-redis": "^4.3.3",
    "mysql2": "^3.11.4",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2"
  }
}
